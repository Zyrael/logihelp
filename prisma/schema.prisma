generator client {
  provider = "prisma-client-js"
  // engineType    = "library"
  // binaryTargets = ["native", "windows"]
  // output        = "../release/app/node_modules/@prisma/client"
}

datasource db {
  provider = "sqlite"
  url      = ""
}

model Supplier {
  id             Int     @id @default(autoincrement())
  name           String
  url            String?
  address        String?
  contacts       String?
  additionalData String?
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}
