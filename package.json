{
  "private": true,
  "name": "logihelp",
  "repository": "git@github.com:Zyrael/logihelp.git",
  "author": "zyrael <zyrael@icloud.com>",
  "license": "MIT",
  "scripts": {
    "install:server": "cd ./server && yarn",
    "install:front": "cd ./front && yarn",
    "postinstall": "yarn run install:server && yarn run install:front",
    "server": "cd ./server && yarn run serve",
    "dev": "cd ./front && yarn run dev",
    "prod": "serve ./front/dist",
    "start:dev": "yarn concurrently \"yarn run server\" \"yarn run dev\"",
    "build:front": "cd ./front && yarn run build",
    "start:prod": "yarn concurrently \"yarn run server\" \"yarn run prod\"",
    "generate": "cd ./server && yarn prisma generate"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.8.3"
  }
}
